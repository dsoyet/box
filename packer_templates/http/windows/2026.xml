                </SynchronousCommand>
                <SynchronousCommand wcm:action="add">
                    <Order>24</Order>
                    <CommandLine>%windir%\System32\cmd.exe /c wmic useraccount where "name='vagrant'" set PasswordExpires=FALSE</CommandLine>
